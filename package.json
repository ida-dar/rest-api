{
  "license": "MIT",
  "version": "1.0.0",
  "engines": {
    "node": "14.17"
  },
  "scripts": {
    "start": "nodemon server.js",
    "importFiles": "mongoimport --uri 'mongodb://root:<PASSWORD>@atlas-host1:27017,atlas-host2:27017,atlas-host3:27017/<DATABASE>?ssl=true&replicaSet=myAtlasRS&authSource=admin' --collection myData --drop --file /somedir/myFileToImport.json",
    "build": "cd client && yarn install && yarn build",
    "test": "set NODE_ENV=test&& mocha \"./{,!(node_modules|client)/**/}*.test.js\" --timeout 4000",
    "test:watch": "onchange \"./**/*.js\" -i -k -- yarn test"
  },
  "dependencies": {
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mocha": "6.2.1",
    "mongodb": "3.3.2",
    "mongoimport": "^1.0.6",
    "mongoose": "5.7.1",
    "onchange": "6.1.0",
    "socket.io": "^4.2.0",
    "uuid": "^8.3.2"
  }
}
